{
  "name": "macgyver-ui-packages",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist && lerna run --parallel flow-typed -- install && lerna run --parallel build",
    "clean": "lerna clean && lerna run --parallel clean -- -s",
    "flow": "lerna run flow -- -s",
    "lint": "lerna run lint -- -s",
    "prettier": "lerna run prettier -- -s",
    "storybook": "start-storybook -p 9002 -c .storybook",
    "test": "lerna run test -- -s",
    "report": "lerna run report -- -s"
  },
  "dependencies": {
    "@storybook/addon-info": "^3.2.18",
    "@storybook/addon-knobs": "^3.2.18",
    "@storybook/addon-options": "^3.3.3",
    "@storybook/react": "^3.2.17",
    "babel-core": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.61.0",
    "lerna": "^2.8.0"
  }
}
