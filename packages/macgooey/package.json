{
  "name": "@macgyver-team/macgooey",
  "version": "0.0.6",
  "description": "The MacGyver UI Library Project",
  "licence": "Apache License 2.0",
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run -s clean && node tools/build",
    "clean": "rimraf dist lib es",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "eslint src/**/*.js",
    "prettier": "prettier -l 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "watch": "npm run -s clean && cross-env BABEL_ENV=es babel src --watch -d es --ignore **/__tests__",
    "test": "jest",
    "test:watch": "jest --watch",
    "report": "coveralls",
    "prepublishOnly": "npm run -s build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "jest": {
    "setupFiles": [
      "./test/bootstrap.js"
    ],
    "testMatch": [
      "**/src/**/__tests__/**/*.test.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "author": "MacGyver Team",
  "contributors": [
    "Tom Lingham <tjlingham@gmail.com>"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.36",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "@babel/preset-flow": "^7.0.0-beta.34",
    "@babel/preset-react": "^7.0.0-beta.34",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-styled-components": "^1.3.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.3",
    "gzip-size": "^4.1.0",
    "jest": "^22.0.1",
    "jest-styled-components": "^4.9.0",
    "prettier": "^1.9.2",
    "pretty-bytes": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "xml2js": "^0.4.19"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "polished": "^1.9.0",
    "ramda": "^0.25.0",
    "recompose": "^0.26.0",
    "styled-components": "^2.2.4"
  }
}
